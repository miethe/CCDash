      "type": "timeseries",
      "title": "Ingestion Throughput",
      "id": 1,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "sum by (entity) (rate(ccdash_ingestion_events_total{project=~\"$project\"}[5m]))",
          "legendFormat": "{{entity}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Parser Failures",
      "id": 2,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "sum by (parser) (rate(ccdash_parser_failures_total{project=~\"$project\"}[5m]))",
          "legendFormat": "{{parser}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ingestion Latency P95 (ms)",
      "id": 3,
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, entity) (rate(ccdash_ingestion_latency_ms_bucket{project=~\"$project\"}[5m])))",
          "legendFormat": "{{entity}}",
          "refId": "A"
        }
      ]
    }
